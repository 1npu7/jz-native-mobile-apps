<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <string name="app_name">JavaZone</string>
    <string name="title_home">Home</string>
    <string name="title_dashboard">Dashboard</string>
    <string name="title_notifications">Notifications</string>


    <plurals name="schedule_block_subtitle"
             description="Shown in the schedule to indicate how many sessions are available on that particular time slot.">
        <item quantity="one"><xliff:g id="number_of_sessions">%1$d</xliff:g> available session</item>
        <item quantity="other"><xliff:g id="number_of_sessions">%1$d</xliff:g> available sessions</item>
    </plurals>

    <plurals name="session_notification_text"
             description="Shows as a notification on the system bar to alert user that some of their sessions are about to begin.">
        <item quantity="one">and <xliff:g id="num_sessions">%2$d</xliff:g> other starting in <xliff:g id="remaining_time">%1$d</xliff:g> min.</item>
        <item quantity="other">and <xliff:g id="num_sessions">%2$d</xliff:g> others starting in <xliff:g id="remaining_time">%1$d</xliff:g> min.</item>
    </plurals>

    <plurals name="session_notification_ticker"
             description="Shown as a notification on the system bar to alert user that some of their sessions are about to begin.">
        <item quantity="one">%1$d session is about to start.</item>
        <item quantity="other">%1$d sessions are about to start.</item>
    </plurals>

    <plurals name="session_plurals"
             description="Indicates a certain number of sessions.">
        <item quantity="one">%1$d session</item>
        <item quantity="other">%1$d sessions</item>
    </plurals>

    <!-- Title for the notification that alerts the user that some of their sessions are about to begin. -->
    <plurals name="session_notification_title">
        <item quantity="one"><xliff:g id="num_sessions">%2$d</xliff:g> session is starting in <xliff:g id="remaining_time">%1$d</xliff:g> min.</item>
        <item quantity="other"><xliff:g id="num_sessions">%2$d</xliff:g> sessions are starting in <xliff:g id="remaining_time">%1$d</xliff:g> min.</item>
    </plurals>

    <plurals name="duration_hours">
        <item quantity="one">%s hour</item>
        <item quantity="other">%s hours</item>
    </plurals>

    <plurals name="duration_minutes">
        <item quantity="one">%s minute</item>
        <item quantity="other">%s minutes</item>
    </plurals>

