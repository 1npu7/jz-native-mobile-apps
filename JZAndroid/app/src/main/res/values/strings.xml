    <string name="remove_bookmark_hint">Event removed from My schedule</string>
    <string name="bookmark_hint_optout">Got it</string>
    <string name="remove_bookmark">Event removed from My schedule</string>
    <string name="clear_filters">Clear filters</string>
    <string name="navdrawer_item_my_schedule">Schedule</string>


</resources>
